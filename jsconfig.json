{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "baseUrl": "./src",
      "paths": {
        "@common/*": ["./common/*"],
        "@app/*": ["./application/*"],
        "@assets/*": ["./assets/*"],
        "@components/*": ["./components/*"],
        "@layouts/*": ["./layouts/*"],
        "@routes/*": ["./routes/*"],
        "@store/*": ["./store/*"]
      }
    },
    "exclude": ["node_modules"],
    "include": ["src"]
  }
  